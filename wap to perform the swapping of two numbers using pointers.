//wap to perform the swapping of two numbers using pointers.
#include<stdio.h>
int main(){
    int a,b;
    int *p1=&a,*p2=&b;
    printf("Enter two numbers:");
    scanf("%d%d",p1,p2);
    printf("Before swapping a=%d and b=%d\n",*p1,*p2);
    int c=*p1;
    *p1=*p2;
    *p2=c;
    printf("After swapping a=%d and b=%d",*p1,*p2);
    return 0;
}
