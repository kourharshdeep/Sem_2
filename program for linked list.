#include<stdio.h>
#include<stdlib.h>
struct Node {
int data;
struct Node *next;
};
int main() {
struct Node *t=(struct Node*)malloc(sizeof(struct Node));
printf("Enter data: ");
scanf("%d",&t->data);
t->next=NULL;

struct Node *t1=(struct Node*)malloc(sizeof(struct Node));
printf("Enter data: ");
scanf("%d",&t1->data);
t1->next=NULL;
t->next=t2;

struct Node *t2=(struct Node*)malloc(sizeof(struct Node));
printf("Enter data: ");
scanf("%d",&t2->data);
t2->next=NULL;
t1->next=t2;

printf("%d->%d->%d",t->data,t1->data,t2->data);
return 0;
}
