//wap to display the elements of an array using pointers.
#include<stdio.h>
int main(){
    int arr[5],i;
    printf("Enter the array elments: ");
    for(i=0;i<5;i++){
        scanf("%d",arr+i);
    }
    printf("Array elements are:\n");
    for(i=0;i<5;i++){
        printf("%d\t",*(arr+i));
    }
    return 0;
}
